<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="report_sales_quota">
        <t t-call="web.html_container">
            <t t-call="web.internal_layout">
                <div class="page">
                    <h2>Sales Quota</h2>
                    <br/>
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th class="text-left">
                                    <span>Sales Team</span>
                                </th>
                                <th class="text-left">
                                    <span>Product</span>
                                </th>
                                 <th class="text-left">
                                    <span>Start Date</span>
                                </th>
                                 <th class="text-left">
                                    <span>End Date</span>
                                </th>
                                 <th class="text-left">
                                    <span>Sales Quota</span>
                                </th>
                                 <th class="text-left">
                                    <span>Quantity Sold</span>
                                </th>
                                <th class="text-left">
                                    <span>Quantity Available</span>
                                </th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </t>
        </t>
    </template>
</odoo>